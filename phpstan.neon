includes:
    - vendor/symplify/phpstan-rules/config/symplify-rules.neon

parameters:
    level: max

    paths:
        - config
        - src
        - tests

    excludePaths:
        - */Source/*
        - *Source/*

    ignoreErrors:
        -
            message: '#Attribute must have all names explicitly defined#'
            path: "tests/*"
